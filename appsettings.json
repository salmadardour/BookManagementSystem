{
  "ConnectionStrings": {
    "BookManagementContext": "Data Source=bookmanagement.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKey",  
    "Issuer": "BookManagementSystem",
    "Audience": "BookManagementAPI",
    "ExpiryMinutes": 60
  },
  "Security": {
    "EnableHttps": true,
    "AllowedOrigins": ["https://yourfrontend.com"]
  }
}
